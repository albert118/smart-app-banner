{
    "name": "@easy-smart-app-banner/vue-demo",
    "version": "1.0.0",
    "description": "A Vue demo for smart-app-banner",
    "main": "main.ts",
    "scripts": {
        "dev": "vite",
        "setup": "npm i --install-links && npm run type-check",
        "build": "npm run build-only && npm run type-check",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --project tsconfig.app.json --composite false --noEmit",
        "test": "vitest --watch false",
        "format": "prettier --write src/",
        "clean": "rm -rf ./dist",
        "prepublish": "npm run clean && npm run build"
    },
    "author": "Albert Ferguson <albertferguson118@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/albert118/smart-app-banner/issues"
    },
    "private": false,
    "dependencies": {
        "@easy-smart-app-banner/core": "^1.0.4",
        "@easy-smart-app-banner/vue-plugin": "^1.0.5",
        "@vueuse/core": "^13.0.0",
        "js-logger": "^1.6.1",
        "typescript": "^5.8.2",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.13.9",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vue/compiler-sfc": "^3.5.13",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "happy-dom": "^17.4.6",
        "jsdom": "^26.0.0",
        "prettier": "3.5.3",
        "sass-embedded": "^1.87.0",
        "typescript": "~5.8.0",
        "unplugin-auto-import": "^19.2.0",
        "vite": "^6.3.5",
        "vite-plugin-vue-devtools": "^7.7.6",
        "vitest": "^3.1.3",
        "vue-tsc": "^2.2.10"
    }
}
